<Window x:Class="TeamsManager.Views.MainWindowView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeamsManager.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
            <Button Content="Welcome" Click="WelcomeButton_Click"/>
            <Button Content="Teams List" Click="TeamsListButton_Click"/>
        </StackPanel>
        <Frame x:Name="Main" Margin="0,35,0,0"/>
    </Grid>
</Window>
